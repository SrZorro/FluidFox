    "inferno": "^6.1.3",
    "node-uuid": "^1.4.8",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/node": "^10.12.2",
    "@types/node-uuid": "0.0.28",
    "@types/ws": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "ts-transform-inferno": "^4.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "typestyle": "^2.0.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
